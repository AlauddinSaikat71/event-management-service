{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "ems/ems-library/(.*)": "<rootDir>/../libs/ems-library/src/$1",
    "ems/ems-library": "<rootDir>/../libs/ems-library/src",
    "ems/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "ems/common": "<rootDir>/../libs/common/src",
    "@app/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@app/common": "<rootDir>/../libs/common/src"
  }
}